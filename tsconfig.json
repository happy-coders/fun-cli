{
  "include": [
   "./src"
  ],
  "exclude": [
    "node_modules",
    "e2e"
  ],
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "dom",
      "esnext"
    ],
    "importHelpers": true,
    "declaration": true,
    "sourceMap": false,
    "rootDir": "./src",
    "outDir": "dist",
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitThis": true,
  }
}